{
  "name": "harmonized-vscode-theme",
  "displayName": "Harmonized",
  "description": "Solarized in harmony",
  "version": "0.2.1",
  "scripts": {
    "release:major": "yarn build && vsce publish major && git push",
    "release:minor": "yarn build && vsce publish minor && git push",
    "release:patch": "yarn build && vsce publish patch && git push",
    "build": "node src/index.js"
  },
  "preview": false,
  "publisher": "wheredoesyourmindgo",
  "galleryBanner": {
    "color": "#fdf6e3",
    "theme": "light"
  },
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/wheredoesyourmindgo/harmonized-vscode-theme"
  },
  "bugs": {
    "url": "https://github.com/wheredoesyourmindgo/harmonized-vscode-theme/issues"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "solarized",
    "light",
    "dark",
    "zen",
    "contrast"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Harmonized light",
        "uiTheme": "vs",
        "path": "./themes/light.json"
      },
      {
        "label": "Harmonized dark",
        "uiTheme": "vs-dark",
        "path": "./themes/dark.json"
      },
      {
        "label": "Harmonized light _",
        "uiTheme": "vs",
        "path": "./themes/light_.json"
      },
      {
        "label": "Harmonized dark _",
        "uiTheme": "vs-dark",
        "path": "./themes/dark_.json"
      },
      {
        "label": "Harmonized light (hc)",
        "uiTheme": "vs",
        "path": "./themes/light-hc.json"
      },
      {
        "label": "Harmonized dark (hc)",
        "uiTheme": "vs-dark",
        "path": "./themes/dark-hc.json"
      },
      {
        "label": "Harmonized light (hc) _",
        "uiTheme": "vs",
        "path": "./themes/light-hc_.json"
      },
      {
        "label": "Harmonized dark (hc) _",
        "uiTheme": "vs-dark",
        "path": "./themes/dark-hc_.json"
      }
    ]
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.56.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.56.0",
    "node-fetch": "^3.1.1",
    "strip-json-comments": "^3.1.1",
    "vsce": "^1.88.0"
  }
}
